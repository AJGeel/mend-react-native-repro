{
  "expo": {
    "name": "repro-repo",
    "slug": "repro-repo",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFF",
          "image": "./assets/splash-icon-light.png",
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.ajgeel.repro-repo"
    }
  }
}
